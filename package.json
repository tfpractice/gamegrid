{
  "name": "game_grid",
  "version": "1.0.0",
  "description": "grid",
  "main": "dist/bundle.umd.js",
  "module": "dist/bundle.es6.js",
  "modules.root": "src",
    "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn run build:umd && yarn run build:es6",
    "build:es6": "NODE_ENV=production rollup -c rollup.es6.config.js",
    "build:umd": "NODE_ENV=production rollup -c rollup.config.js",
    "build:dev": "rollup -c rollup.es6.config.js",
    "coverage": "yarn test && cat coverage/lcov.info | coveralls",
    "start": "webpack --config config/webpack.config.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tfpractice//gamegrid.git"
  },
  "keywords": [
    "game"
  ],
  "author": "theory f practice <jackson.brebnor@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-import-resolver-babel-module": "^2.2.1",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "jasmine": "^2.5.2",
    "jasmine-expect": "^3.0.1",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.40.2",
    "rollup-plugin-alias": "^1.2.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-filesize": "^1.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-progress": "^0.2.1",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-plugin-visualizer": "^0.1.5",
    "webpack": "^1.13.2"
  },
  "jest": {
    "collectCoverage": "true",
    "collectCoverageFrom": [
      "src/**"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathDirs": [
      "spec/"
    ],
    "unmockedModulePathPatterns": [
      "jasmine-expect"
    ]
  },
  "dependencies": {
    "core-js": "^2.4.1",
    "functional_graph_theory": "git://github.com/tfpractice/functional_graph_theory.git"
  }
}
